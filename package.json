{
  "name": "s3-memory-server",
  "version": "0.0.4",
  "description": "",
  "bin": "./build/cjs/index.js",
  "main": "./build/cjs/index.js",
  "module": "./build/esm/index.js",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "build:esm": "tsc --project ./tsconfig.esm.json",
    "start": "ts-node-dev --respawn -- ./src/index.ts",
    "test": "jest"
  },
  "keywords": [
    "api",
    "amazon",
    "minio",
    "cloud",
    "s3",
    "storage"
  ],
  "author": {
    "name": "Arthur Simas",
    "url": "https://arthursimas.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arthursimas1/s3-memory-server.git"
  },
  "homepage": "https://github.com/arthursimas1/s3-memory-server",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "11.1.1",
    "jest": "29.6.4",
    "minio": "7.1.3",
    "nanoid": "3.3.6",
    "tslib": "2.6.2"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.1",
    "@types/jest": "29.5.4",
    "@types/node": "20.6.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.2.2"
  },
  "exports": {
    ".": {
      "require": "./build/cjs/index.js",
      "default": "./build/esm/index.js"
    },
    "./build/cjs/internal/*": null,
    "./build/cjs/*": {
      "require": "./build/cjs/*",
      "default": null
    },
    "./build/esm/internal/*": null,
    "./build/esm/*": {
      "import": "./build/esm/*",
      "default": null
    },
    "./package.json": "./package.json"
  },
  "files": [
    "./package.json",
    "./build/",
    "./src/",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.3.0"
  },
  "packageManager": "yarn@3.6.3"
}
